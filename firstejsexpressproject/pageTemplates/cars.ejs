    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/styles/styles.css">
    <title><%= title %></title>
</head>
<body>
    <h1> table car </h1>
    <table>
        <tr>
            <th>model</th>
            <th>license</th>
            <th>year</th>
        </tr>
        <tr>
            <% cars.forEach(car => { 
                %> <tr>
                    <td>
                        <%= car.model %>
